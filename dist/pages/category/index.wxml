<block wx:if="{{$taroCompReady}}">
    <at-tabs compid="{{$compid__164}}">
        <at-tabs-pane compid="{{$compid__165}}">
            <view class="list">
                <view class="list-item" wx:key="n" wx:for="{{loopArray85}}" wx:for-item="m" wx:for-index="n">
                    <view class="list-item-top">
                        <view class="left">
                            <image class="pig-img" src="{{m.$loopState__temp2}}"></image><text class="title">#{{categorys[current].title}}#</text>
                        </view>
                        <view class="right"><text class="copy" bindtap="anonymousFunc0" data-e-tap-so="this" data-e-tap-a-a="{{m._$indexKey}}">一键复制</text>
                        </view>
                    </view>
                    <rich-text class="list-item-txt" nodes="{{m.$loopState__temp4}}"></rich-text>
                    <view class="tips">{{m.$original.author + (m.$original.origin ? '《' + m.$original.origin + '》' : '')}}</view>
                </view>
            </view>
        </at-tabs-pane>
    </at-tabs>
</block>